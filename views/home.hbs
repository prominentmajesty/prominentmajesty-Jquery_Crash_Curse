{{> header}}
<div class="container">
    <ul class="list-group" id="list"></ul>
</div>
<div class="container-fluid" style="margin: 20px 0px 0px 20px">
    <div class="container">
        <form class="form1" id="form1" enctype="multipart/form-data">
            <div class="form-group row">
                <label for="name" class="col-sm-2 col-form-label col-form-label-lg">Name :</label>
                <div class="col-sm-10">
                    <input type="text" name="name" class="form-control form-control-lg" id="name" placeholder="Name">
                </div>
            </div>
            <div class="form-group row">
                <label for="email" class="col-sm-2 col-form-label col-form-label-lg">Email :</label>
                <div class="col-sm-10">
                    <input type="email" name="email" class="form-control form-control-lg" id="email" placeholder="Email">
                </div>
            </div> 
            <div class="form-group row">
                <label for="password" class="col-sm-2 col-form-label col-form-label-lg">Password :</label>
                <div class="col-sm-10">
                    <input type="password" name="password" class="form-control form-control-lg" id="password" placeholder="password">
                </div>
            </div>
            <div class="classButton">
                <button type="button" id = "button" class="btn btn-primary btn-lg btn-block">Sumit request...</button>
            </div>
        </form>
    </div>
</div>
